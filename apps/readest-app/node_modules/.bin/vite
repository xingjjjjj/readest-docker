#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/app/readest/node_modules/.pnpm/vite@7.2.6_@types+node@22.15.31_jiti@1.21.7_terser@5.44.0_yaml@2.8.1/node_modules/vite/bin/node_modules:/app/readest/node_modules/.pnpm/vite@7.2.6_@types+node@22.15.31_jiti@1.21.7_terser@5.44.0_yaml@2.8.1/node_modules/vite/node_modules:/app/readest/node_modules/.pnpm/vite@7.2.6_@types+node@22.15.31_jiti@1.21.7_terser@5.44.0_yaml@2.8.1/node_modules:/app/readest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/app/readest/node_modules/.pnpm/vite@7.2.6_@types+node@22.15.31_jiti@1.21.7_terser@5.44.0_yaml@2.8.1/node_modules/vite/bin/node_modules:/app/readest/node_modules/.pnpm/vite@7.2.6_@types+node@22.15.31_jiti@1.21.7_terser@5.44.0_yaml@2.8.1/node_modules/vite/node_modules:/app/readest/node_modules/.pnpm/vite@7.2.6_@types+node@22.15.31_jiti@1.21.7_terser@5.44.0_yaml@2.8.1/node_modules:/app/readest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@7.2.6_@types+node@22.15.31_jiti@1.21.7_terser@5.44.0_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@7.2.6_@types+node@22.15.31_jiti@1.21.7_terser@5.44.0_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
fi
