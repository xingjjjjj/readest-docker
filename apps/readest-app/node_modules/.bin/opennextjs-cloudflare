#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/app/readest/node_modules/.pnpm/@opennextjs+cloudflare@1.14.7_next@16.0.10_@babel+core@7.28.0_react-dom@19.2.0_react@19_8df517659a39ca68ea85af9aab61cfc7/node_modules/@opennextjs/cloudflare/dist/cli/node_modules:/app/readest/node_modules/.pnpm/@opennextjs+cloudflare@1.14.7_next@16.0.10_@babel+core@7.28.0_react-dom@19.2.0_react@19_8df517659a39ca68ea85af9aab61cfc7/node_modules/@opennextjs/cloudflare/dist/node_modules:/app/readest/node_modules/.pnpm/@opennextjs+cloudflare@1.14.7_next@16.0.10_@babel+core@7.28.0_react-dom@19.2.0_react@19_8df517659a39ca68ea85af9aab61cfc7/node_modules/@opennextjs/cloudflare/node_modules:/app/readest/node_modules/.pnpm/@opennextjs+cloudflare@1.14.7_next@16.0.10_@babel+core@7.28.0_react-dom@19.2.0_react@19_8df517659a39ca68ea85af9aab61cfc7/node_modules/@opennextjs/node_modules:/app/readest/node_modules/.pnpm/@opennextjs+cloudflare@1.14.7_next@16.0.10_@babel+core@7.28.0_react-dom@19.2.0_react@19_8df517659a39ca68ea85af9aab61cfc7/node_modules:/app/readest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/app/readest/node_modules/.pnpm/@opennextjs+cloudflare@1.14.7_next@16.0.10_@babel+core@7.28.0_react-dom@19.2.0_react@19_8df517659a39ca68ea85af9aab61cfc7/node_modules/@opennextjs/cloudflare/dist/cli/node_modules:/app/readest/node_modules/.pnpm/@opennextjs+cloudflare@1.14.7_next@16.0.10_@babel+core@7.28.0_react-dom@19.2.0_react@19_8df517659a39ca68ea85af9aab61cfc7/node_modules/@opennextjs/cloudflare/dist/node_modules:/app/readest/node_modules/.pnpm/@opennextjs+cloudflare@1.14.7_next@16.0.10_@babel+core@7.28.0_react-dom@19.2.0_react@19_8df517659a39ca68ea85af9aab61cfc7/node_modules/@opennextjs/cloudflare/node_modules:/app/readest/node_modules/.pnpm/@opennextjs+cloudflare@1.14.7_next@16.0.10_@babel+core@7.28.0_react-dom@19.2.0_react@19_8df517659a39ca68ea85af9aab61cfc7/node_modules/@opennextjs/node_modules:/app/readest/node_modules/.pnpm/@opennextjs+cloudflare@1.14.7_next@16.0.10_@babel+core@7.28.0_react-dom@19.2.0_react@19_8df517659a39ca68ea85af9aab61cfc7/node_modules:/app/readest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@opennextjs/cloudflare/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../@opennextjs/cloudflare/dist/cli/index.js" "$@"
fi
